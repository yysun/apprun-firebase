{
  "name": "apprun-starter",
  "version": "1.0.0",
  "description": "AppRun Application Starter Template",
  "main": "index.html",
  "scripts": {
    "test": "jest --watch",
    "start": "run-p start:*",
    "start:dev": "webpack-dev-server --mode development",
    "start:pages": "apprun-site build -w -V",
    "build": "run-s build:*",
    "build:pages": "apprun-site build",
    "build:js": "webpack -p --mode production"
  },
  "keywords": [
    "AppRUn",
    "AppRun Site",
    "Template"
  ],
  "author": "Yiyi Sun <yiyisun@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apprunjs/apprun-starter.git"
  },
  "bugs": {
    "url": "https://github.com/apprunjs/apprun-starter/issues"
  },
  "homepage": "https://github.com/apprunjs/apprun-starter#readme",
  "devDependencies": {
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "apprun": "^2.20.6",
    "apprun-site": "^0.5.2",
    "material-components-web": "^2.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
