{
  "name": "apprun-starter",
  "version": "1.0.0",
  "description": "AppRun Application Starter Template",
  "main": "index.html",
  "scripts": {
    "test": "jest --watch",
    "start": "webpack-dev-server --mode development",
    "build": "webpack -p --mode production",
    "install": "npm install -D apprun source-map-loader ts-loader typescript webpack webpack-cli webpack-dev-server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "apprun": "^2.19.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.1",
    "typescript": "^3.4.5",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {},
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
